{
    "name": "adz-chappers/iconicapp-for-laravel",
    "description": "Laravel Blade files for the wonderful iconic.app icon set",
    "keywords": [
        "adzchappers",
        "laravel",
        "iconicapp-for-laravel",
        "iconic app",
        "iconic.app",
        "Blade Icons",
        "Blade Iconic.app"
    ],
    "homepage": "https://github.com/adz-chappers/iconicapp-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "AdzChappers",
            "email": "adam.php.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "AdzChappers\\IconicappForLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdzChappers\\IconicappForLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdzChappers\\IconicappForLaravel\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
